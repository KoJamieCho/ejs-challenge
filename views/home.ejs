<%- include('partials/header'); -%>
    <h1>Home</h1>
    <p> <%=startingContent%> </p>    
    <% posts.forEach(function(val) { %>
        <h1> <%= val.title %></h1>
        <p>
            <%= val.content.substring(0,100) + "..." %> 
            <a href="/posts/<%=val.title%>">Read More</a> 
        </p>
        
    <% }); %>

<%- include('partials/footer'); -%>
